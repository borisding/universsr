{
  "name": "universsr",
  "version": "7.0.0",
  "description": "Universal React + Redux project starter kit.",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf public app/build",
    "webpack": "webpack --hide-modules",
    "build": "cross-env NODE_ENV=production npm-run-all clean webpack",
    "build:analyze": "cross-env ANALYZE_MODE=enabled npm run build",
    "dev": "run-p dev:api dev:app",
    "dev:app": "cross-env NODE_ENV=development node app",
    "dev:api": "cross-env NODE_ENV=development nodemon api",
    "start": "run-p start:api start:app",
    "start:app": "cross-env NODE_ENV=production node app",
    "start:api": "cross-env NODE_ENV=production node api",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "rimraf assets/coverage && npm test -- --coverage",
    "test:update": "npm test -- --updateSnapshot",
    "test:watch": "npm test -- --watch",
    "lint": "npm-run-all lint:js lint:style",
    "lint:style": "stylelint **/*.{css,scss,sass} --fix",
    "lint:js": "eslint . --fix",
    "prepare": "husky install"
  },
  "engines": {
    "npm": ">=6.12.0",
    "node": ">=12.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/borisding/universsr.git"
  },
  "keywords": [
    "universsr",
    "universal",
    "ssr",
    "express",
    "react",
    "redux",
    "react-redux"
  ],
  "author": "Boris Ding Poh Hing",
  "license": "MIT",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "@loadable/component": "^5.15.0",
    "@loadable/server": "^5.15.0",
    "app-root-path": "^3.0.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "compression": "^1.7.4",
    "connected-react-router": "^6.8.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.15.2",
    "cors": "^2.8.5",
    "detect-node": "^2.1.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "history": "^4.10.1",
    "hpp": "^0.2.3",
    "html-minifier": "^4.0.0",
    "http-proxy-middleware": "^1.0.5",
    "make-promises-safe": "^5.1.0",
    "morgan": "^1.10.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-frontload": "^1.1.0",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "serialize-javascript": "^6.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-strict-mode": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.3",
    "css-loader": "^4.2.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.0.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "mini-css-extract-plugin": "^0.9.0",
    "nock": "^13.1.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.8",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^9.0.3",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^4.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.1"
  },
  "nodemonConfig": {
    "debug": true,
    "ext": "js",
    "env": {
      "NODE_ENV": "development"
    },
    "watch": [
      "api/*",
      "middleware/*"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not IE 10"
  ]
}
