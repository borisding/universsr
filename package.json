{
  "name": "universsr",
  "version": "1.0.0-beta.2",
  "description": "Universal React + Redux, Express application boilerplate.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public config/properties.json src/**/*-built.*",
    "build:config": "node build/scripts/config.js",
    "build": "npm-run-all clean build:config && cross-env NODE_ENV=production webpack --config build/webpack --progress --hide-modules",
    "prod:api": "cross-env NODE_ENV=production node api.js",
    "prod:app": "cross-env NODE_ENV=production node app.js",
    "dev:api": "nodemon --exec node api.js",
    "dev:app": "cross-env NODE_ENV=development node app.js",
    "start": "npm-run-all clean build && run-p prod:api prod:app",
    "dev": "npm-run-all clean build:config && run-p dev:api dev:app",
    "lint": "npm-run-all lint:js lint:style",
    "lint:style": "stylelint **/*.scss --syntax=scss --fix",
    "lint:js": "eslint . --fix",
    "precommit": "lint-staged",
    "postinstall": "npm run build",
    "test": "npm run lint:js && cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "rimraf coverage && npm test -- --coverage"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:js",
      "git add"
    ],
    "*.scss": [
      "npm run lint:style",
      "git add"
    ]
  },
  "verbose": true,
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/borisding/universsr.git"
  },
  "keywords": [
    "universsr",
    "universal",
    "ssr",
    "express",
    "react",
    "redux",
    "react-redux"
  ],
  "author": "Boris Ding Poh Hing",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^2.1.0",
    "axios": "^0.18.0",
    "babel-plugin-react-css-modules": "^3.4.2",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "detect-node": "^2.0.3",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "esm": "^3.0.66",
    "express": "^4.16.3",
    "extend": "^3.0.1",
    "getenv": "^0.7.0",
    "helmet": "^3.12.1",
    "hpp": "^0.2.2",
    "http-proxy": "^1.17.0",
    "husky": "^0.14.3",
    "isdev": "^1.0.1",
    "lint-staged": "^7.2.0",
    "make-promises-safe": "^1.1.0",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-s-alert": "^1.4.1",
    "react-universal-component": "^2.9.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "redux-thunk-init": "^1.0.3",
    "regenerator-runtime": "^0.12.0",
    "rimraf": "^2.6.2",
    "serialize-javascript": "^1.5.0",
    "serve-favicon": "^2.5.0",
    "slash": "^2.0.0",
    "uuid": "^3.3.2",
    "webpack-flush-chunks": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/polyfill": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-react": "^7.0.0-beta.52",
    "@babel/preset-stage-2": "^7.0.0-beta.52",
    "autoprefixer": "^8.6.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-universal-import": "^3.0.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.0",
    "enzyme": "^3.3.0",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-prettier": "^2.6.2",
    "extract-css-chunks-webpack-plugin": "^3.0.6",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.3.0",
    "node-sass": "^4.9.2",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "offline-plugin": "^5.0.5",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "postcss-scss": "^1.0.6",
    "prettier": "^1.13.7",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.0.3",
    "stats-webpack-plugin": "^0.6.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.1.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-hot-server-middleware": "^0.5.0"
  },
  "nodemonConfig": {
    "watch": [
      "src/api/*"
    ],
    "debug": true,
    "ext": "js jsx",
    "env": {
      "NODE_ENV": "development"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/resources/mocks/file-mock.js",
      "\\.(css|scss)$": "<rootDir>/resources/mocks/style-mock.js"
    },
    "testEnvironment": "node",
    "testRegex": ".*.spec\\.js$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "_moduleAliases": {
    "@root": ".",
    "@bin": "bin",
    "@build": "build",
    "@public": "public",
    "@resources": "resources",
    "@assets": "resources/assets",
    "@config": "resources/config",
    "@utils": "src/utils",
    "@api": "src/api",
    "@app": "src/app",
    "@common": "src/app/common",
    "@modules": "src/app/modules",
    "@redux": "src/app/redux",
    "@middlewares": "src/middlewares"
  }
}
