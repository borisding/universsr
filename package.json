{
  "name": "universsr",
  "version": "6.0.0",
  "description": "Universal React + Redux project starter kit.",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf public src/server/dist",
    "webpack": "webpack --hide-modules",
    "build": "cross-env NODE_ENV=production npm-run-all clean webpack",
    "build:analyze": "cross-env ANALYZE_MODE=enabled npm run build",
    "dev:app": "cross-env NODE_ENV=development node app.js",
    "dev:api": "nodemon --exec node api.js",
    "dev": "run-p dev:api dev:app",
    "start:app": "cross-env NODE_ENV=production node app.js",
    "start:api": "cross-env NODE_ENV=production node api.js",
    "start": "run-p start:api start:app",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:coverage": "rimraf assets/coverage && npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "lint": "npm-run-all lint:js lint:style",
    "lint:style": "stylelint **/*.scss --syntax=scss --fix",
    "lint:js": "eslint . --fix",
    "heroku-postbuild": "npm run build"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "verbose": true,
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=8.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/borisding/universsr.git"
  },
  "keywords": [
    "universsr",
    "universal",
    "ssr",
    "express",
    "react",
    "redux",
    "react-redux"
  ],
  "author": "Boris Ding Poh Hing",
  "license": "MIT",
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "app-root-path": "^3.0.0",
    "axios": "^0.19.2",
    "babel-plugin-react-css-modules": "^5.2.6",
    "classnames": "^2.2.6",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "connected-react-router": "^6.6.1",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "detect-node": "^2.0.4",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "history": "^4.10.1",
    "hpp": "^0.2.3",
    "html-minifier": "^4.0.0",
    "http-proxy": "^1.18.0",
    "husky": "^4.2.1",
    "is-plain-object": "^3.0.0",
    "make-promises-safe": "^5.1.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "nock": "^11.7.2",
    "open": "^7.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-frontload": "^1.1.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.19",
    "react-redux": "^7.1.3",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-s-alert": "^1.4.1",
    "react-universal-component": "^4.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.1",
    "serialize-javascript": "^2.1.2",
    "slash": "^3.0.0",
    "uuid": "^3.4.0",
    "webpack-flush-chunks": "^2.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-strict-mode": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-universal-import": "^4.0.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "stats-webpack-plugin": "^0.7.0",
    "stylelint": "^13.0.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.14.2",
    "terser-webpack-plugin": "^2.3.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.0"
  },
  "nodemonConfig": {
    "debug": true,
    "ext": "js",
    "env": {
      "NODE_ENV": "development"
    },
    "watch": [
      "api/*",
      "src/server/middleware/*"
    ]
  },
  "localScopedName": "[local]___[hash:base64:5]",
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "eslintIgnore": [
    "public/**/*.js",
    "src/server/dist"
  ],
  "_moduleAliases": {
    "@root": ".",
    "@api": "api",
    "@assets":  "assets",
    "@client": "src/client",
    "@config": "config",
    "@public": "public",
    "@server": "src/server",
    "@utils": "src/utils"
  }
}
