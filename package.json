{
  "name": "universsr",
  "version": "8.0.0",
  "description": "Universal React web app boilerplate.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && cross-env NODE_ENV=production webpack",
    "build:analyze": "cross-env ANALYZE_MODE=true npm run build",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "start": "cross-env NODE_ENV=production node index.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "rimraf resources/coverage && npm test -- --coverage",
    "test:update": "npm test -- --updateSnapshot",
    "test:watch": "npm test -- --watch",
    "lint": "npm-run-all lint:js lint:style",
    "lint:style": "stylelint **/*.{css,scss,sass} --fix",
    "lint:js": "eslint . --fix",
    "prepare": "husky install"
  },
  "engines": {
    "npm": ">=6.14.8",
    "node": ">=14.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/borisding/universsr.git"
  },
  "keywords": [
    "universsr",
    "universal",
    "ssr",
    "express",
    "react"
  ],
  "author": "Boris Ding Poh Hing",
  "license": "MIT",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "@loadable/server": "^5.15.3",
    "axios": "^1.3.3",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "core-js": "^3.28.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "esm-wallaby": "^3.2.26",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "html-minifier": "^4.0.0",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-frontload": "^2.0.0",
    "react-helmet-async": "^1.3.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.3.0",
    "rimraf": "^4.1.2",
    "serialize-javascript": "^6.0.1",
    "serve-favicon": "^2.5.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-strict-mode": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@loadable/babel-plugin": "^5.15.3",
    "@loadable/webpack-plugin": "^5.15.2",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.4.3",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.3",
    "mini-css-extract-plugin": "^2.7.2",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.4",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.58.1",
    "sass-loader": "^13.2.0",
    "stylelint": "^15.1.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^6.0.1",
    "webpack-hot-middleware": "^2.25.3",
    "webpack-hot-server-middleware": "^0.6.1"
  },
  "nodemonConfig": {
    "debug": true,
    "ext": "js",
    "env": {
      "NODE_ENV": "development"
    },
    "ignore": [
      "app/middleware/serverRenderer.js"
    ],
    "watch": [
      "app/logger/*",
      "app/middleware/*",
      "app/index.js",
      "app/server.js"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not IE 10"
  ]
}
